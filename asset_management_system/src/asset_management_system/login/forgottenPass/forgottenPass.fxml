<AnchorPane id="AnchorPane" prefHeight="563.0" prefWidth="738.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asset_management_system.login.forgottenPass.ForgottenPassController">
