<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asset_management_system.dashboard.DashboardController">
   <children>
      <VBox fx:id="vbox" prefHeight="900.0" prefWidth="240.0" style="-fx-background-color: #28d474; -fx-padding: 10; -fx-spacing: 8;" />
      <AnchorPane fx:id="pieChart_pane" layoutX="263.0" layoutY="470.0" prefHeight="340.0" prefWidth="400.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <PieChart fx:id="catChart" layoutX="-1.0" prefHeight="340.0" prefWidth="400.0" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="tableView_pane" layoutX="797.0" layoutY="470.0" prefHeight="340.0" prefWidth="654.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <TableView fx:id="tbleView_dash" layoutX="1.0" prefHeight="340.0" prefWidth="654.0">
              <columns>
                <TableColumn fx:id="assetName_clmn" prefWidth="101.60003662109375" text="ASSET NAME" />
                <TableColumn fx:id="assetCode_clmn" prefWidth="78.4000244140625" text="CODE" />
                  <TableColumn fx:id="addedOn_clmn" prefWidth="101.5999755859375" text="ADDED ON" />
                  <TableColumn fx:id="addedBy_clmn" prefWidth="127.19989013671875" text="ADDED BY" />
                  <TableColumn fx:id="category_clmn" prefWidth="125.5999755859375" text="CATEGORY" />
                  <TableColumn fx:id="cost_clmn" prefWidth="121.5999755859375" text="COST" />
              </columns>
            </TableView>
         </children></AnchorPane>
      <VBox fx:id="assets_pane" alignment="CENTER" layoutX="263.0" layoutY="173.0" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <Label fx:id="assetsLbl" prefHeight="30.0" prefWidth="77.0" text="ASSETS">
               <font>
                  <Font name="Aparajita" size="20.0" />
               </font>
            </Label>
            <Label fx:id="assets_rst" prefHeight="30.0" prefWidth="54.0" text="Label" />
            <JFXButton fx:id="asset_btn" buttonType="RAISED" prefHeight="30.0" prefWidth="64.0" text="more " />
         </children>
      </VBox>
      <VBox fx:id="totalCost_pane" alignment="CENTER" layoutX="564.0" layoutY="173.0" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <Label fx:id="totalCostLbl" prefHeight="30.0" prefWidth="102.0" text="TOTAL COST">
               <font>
                  <Font name="Aparajita" size="20.0" />
               </font>
            </Label>
            <Label fx:id="cost_rst" prefHeight="30.0" prefWidth="54.0" text="Label" />
            <JFXButton fx:id="totalCost_btn" buttonType="RAISED" prefHeight="30.0" prefWidth="78.0" text="more" textAlignment="CENTER" />
         </children>
      </VBox>
      <VBox fx:id="workers_pane" alignment="CENTER" layoutX="874.0" layoutY="173.0" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <Label fx:id="workersLbl" prefHeight="30.0" prefWidth="93.0" text="WORKERS">
               <font>
                  <Font name="Aparajita" size="20.0" />
               </font>
            </Label>
            <Label fx:id="workers_rst" prefHeight="30.0" prefWidth="54.0" text="Label" />
            <JFXButton fx:id="workers_btn" buttonType="RAISED" onAction="#goToWorkers" prefHeight="30.0" prefWidth="110.0" text="more" textAlignment="CENTER" />
         </children>
      </VBox>
      <VBox fx:id="withUsers_pane" alignment="CENTER" layoutX="1202.0" layoutY="173.0" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #c9f5f5;">
         <children>
            <Label fx:id="withWorkersLbl" prefHeight="30.0" prefWidth="190.0" text="ASSETS WITH WORKERS" textAlignment="CENTER">
               <font>
                  <Font name="Aparajita" size="20.0" />
               </font>
            </Label>
            <Label fx:id="withWorkers_rst" prefHeight="30.0" prefWidth="54.0" text="Label" />
            <JFXButton fx:id="assetWorkers_btn" buttonType="RAISED" prefHeight="30.0" prefWidth="78.0" text="more" textAlignment="CENTER" />
         </children>
      </VBox>
      <ImageView fx:id="backToDashboard" fitHeight="38.0" fitWidth="44.0" layoutX="274.0" layoutY="100.0" onMousePressed="#imageClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/ic_exit_to_app.imageset/ic_exit_to_app_2x.png" />
         </image>
      </ImageView>
      <ImageView fx:id="closeApp" fitHeight="38.0" fitWidth="44.0" layoutX="1414.0" layoutY="14.0" onMousePressed="#closeAppWindow" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/ic_close.imageset/ic_close_2x.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
